@{
    var plano1 = (CEGA.Models.Plano)ViewData["Plano1"];
    var plano2 = (CEGA.Models.Plano)ViewData["Plano2"];
    ViewData["Title"] = "Resultado de Comparación";
}

<h2>Comparación de Planos</h2>

<div class="row">
    <div class="col-md-6">
        <h5>@plano1.NombreArchivo</h5>
    </div>
    <div class="col-md-6">
        <h5>@plano2.NombreArchivo</h5>
    </div>
</div>

<a asp-action="CompararPlanos" class="btn btn-secondary mt-3">Volver</a>
